					{
						"name": "((application.name))",
						"downloads": [],
						"files": [
							"((application.name)).zip"
						]
					}
				],
				"instance_tags": [
					{
						"name": "application.name",
						"value": "((application.name))"
					}
				],
				"post_deployment_actions": {
					"loadbalancer": {}
				},
				"cache_info": {},
