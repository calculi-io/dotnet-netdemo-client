					{
						"name": "netdemo-client",
						"steps": [
							{
								"name": "Restore",
								"definition": "Restore with nuget",
								"command": "nuget restore ",
								"properties": null
							},
							{
								"name": "Build",
								"definition": "Build debug files",
								"command": "msbuild /t:Rebuild /p:Configuration=Debug",
								"properties": null
							},
							{
								"name": "Test",
								"definition": "Run Tests",
								"command": ".\\test.ps1",
								"properties": null
							},
							{
								"name": "Make Cobertura Report",
								"definition": "convert report to cobertura format",
								"command": ".\\makereport.ps1",
								"properties": null
							},
							{
								"name": "Package Client",
								"definition": "Pacakge as zip file",
								"command": ".\\package.ps1",
								"properties": null
							},
							{
								"name": "wrong command",
								"definition": "wrong command",
								"command": "wrong command",
								"properties": null
							}
						]
					}
				]
			},
			{
				"name": "Integration",
				"segment_type": "integration",
				"persistent": true,
				"run_after": [
					"Build"
				],
				"exit_actions": {
					"default": {
						"fetch_logs": true,
						"code_analysis": {
							"properties": null
						},
						"delete_instances": true
					}
				},
				"properties": [
					{
						"name": "consul.servicename",
